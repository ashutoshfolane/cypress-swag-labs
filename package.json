{
  "name": "cypress-swag-labs",
  "version": "1.0.0",
  "private": true,
  "description": "Cypress + TypeScript automation framework (example: Swag Labs)",
  "engines": {
    "node": ">=20.11.0 <21"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:spec": "cypress run",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "cy:clean:reports": "rimraf reports/mochawesome reports/junit",
    "cy:clean:artifacts": "rimraf cypress/videos cypress/screenshots cypress/downloads",
    "cy:clean:all": "npm run cy:clean:reports && npm run cy:clean:artifacts",
    "cy:run:clean": "npm run cy:clean:all && cypress run",
    "validate": "npm run lint && npm run typecheck && npm run format:check"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^14.0.0",
    "cypress-mochawesome-reporter": "^3.8.0",
    "cypress-multi-reporters": "^2.0.0",
    "dotenv": "^17.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-cypress": "^5.0.0",
    "mocha-junit-reporter": "^2.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.50.1",
    "zod": "^4.0.0"
  }
}
